<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="SelectDepRtypeMapper">
  <select id="examCount" resultType="_int">
  select count(*) cnt
       from exam_rslt
  </select>
  
  <select id="examDate" resultType="date">
   select sysdate dt from dual
  </select>
  
  
  
</mapper>